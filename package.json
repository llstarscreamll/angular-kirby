  "scripts": {
    "ng": "ng",
    "start": "npm run postinstall.web && ng serve pascal",
    "build": "npm run postinstall.web && ng build pascal",
    "test": "npm run postinstall.web && ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "npm run postinstall.web && ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "postinstall": "electron-rebuild install-app-deps",
    "postinstall.electron": "node tools/electron/postinstall",
    "postinstall.web": "node tools/web/postinstall",
    "build.electron.pascal-desktop": "npm run prepare.electron.pascal-desktop && ng build electron-pascal-desktop --prod --base-href ./",
    "build.electron.pascal-desktop.local": "npm run build.electron.pascal-desktop && electron dist/apps/electron-pascal-desktop",
    "build.electron.pascal-desktop.linux": "npm run build.electron.pascal-desktop && cd dist/apps/electron-pascal-desktop && npx electron-builder build --linux",
    "build.electron.pascal-desktop.windows": "npm run build.electron.pascal-desktop && cd dist/apps/electron-pascal-desktop && npx electron-builder build --windows",
    "build.electron.pascal-desktop.mac": "npm run build.electron.pascal-desktop && cd dist/apps/electron-pascal-desktop && npx electron-builder build --mac",
    "prepare.electron.pascal-desktop": "npm run postinstall.electron && tsc -p apps/electron-pascal-desktop/tsconfig.json",
    "serve.electron.pascal-desktop.target": "ng serve electron-pascal-desktop",
    "serve.electron.pascal-desktop": "wait-on http-get://localhost:4200/ && electron apps/electron-pascal-desktop/src --serve",
    "start.electron.pascal-desktop": "npm run prepare.electron.pascal-desktop && npm-run-all -p serve.electron.pascal-desktop.target serve.electron.pascal-desktop",
    "start.pascal": "npm run postinstall.web && ng serve pascal"
  },
  "private": true,
